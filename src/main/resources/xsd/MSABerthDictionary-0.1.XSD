	<xs:complexType name="Berth">
		<xs:sequence>
			<!--泊位代码    BWDM CHAR(7)-->
			<xs:element name="BerthCode" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="7"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--港口代码    GKDM    CHAR(6) -->
			<xs:element name="PortCode" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--泊位名称    BWMC  VARCHAR(40)-->
			<xs:element name="BerthName" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--泊位简称    BWJC  CHAR(40) -->
			<xs:element name="BerthReferred" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--拼音码   PYM VARCHAR(40) -->
			<xs:element name="PhoneticCode" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--泊位深度 BWSD    DECIMAL(8,2) -->
			<xs:element name="BerthDepth" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="Decimal_8.2_empty"/>
				</xs:simpleType>
			</xs:element>
			<!--泊位等级  BWDJ    INT-->
			<xs:element name="BerthRating" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--靠泊长度 BWCD    INT -->
			<xs:element name="BerthingLength" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--限量TEU XLTEU    INT -->
			<xs:element name="LimitedTEU" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--所属码头 SSMT    VARCHAR(40) -->
			<xs:element name="HarbourView" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--所属海事处 SSHSC    CHAR(6) -->
			<xs:element name="BelongsToTheMarineDepartment" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--机构代码 JGDM   CHAR(6) -->
			<xs:element name="AgencyCode" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--是否对外开放SFDWKF   CHAR(1) -->
			<xs:element name="AreOpening" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--固体危货标志   GTWHBZ    CHAR(1) -->
			<xs:element name="SolidDangerousGoodsMark" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--液体危货标志    YTWHBZ  CHAR(1) -->
			<xs:element name="LiquidDangerousGoodsMark" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--是否供油码头   SFGYMT    CHAR(1)-->
			<xs:element name="IsFuelingDock" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--最大容纳船舶数   ZDRNCBS    INT-->
			<xs:element name="MaximumAccommodatingShipsNum" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--吞吐周转时间  TTZZSJ  INT-->
			<xs:element name="ThroughputTurnaroundTime" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--是否码头主确认   SFMTZQR CHAR(1)-->
			<xs:element name="AreDockMasterConfirmation" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--是否黑名单  SFHMD    CHAR(1)-->
			<xs:element name="AreBlacklist" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--有效标志    YXBZ    CHAR(1)-->
			<xs:element name="ValidFlag" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--更新标志  GXBZ    CHAR(1)-->
			<xs:element name="UpdateFlag" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--更新时间   MRUT    DATE-->
			<xs:element name="Updated" type="yyyyMMddHHmmss" minOccurs="1" maxOccurs="1"/>
			<!--信息来源 XXLY  CHAR(7)-->
			<xs:element name="InformationSource" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="7"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--码头航道深度 MTHDSD  DECIMAL(8,2)-->
			<xs:element name="HarbourFairwaDepth" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="Decimal_8.2_empty"/>
				</xs:simpleType>
			</xs:element>
			<!--靠泊限制 KBXZ    VARCHAR(255)-->
			<xs:element name="BerthingRestrictions" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--泊位经度1 BWJD1   FLOAT-->
			<xs:element name="LongitudeBerth1" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--泊位经度2 BWJD2   FLOAT-->
			<xs:element name="LongitudeBerth2" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--泊位纬度1 BWWD1   FLOAT-->
			<xs:element name="LatitudeBerth1" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--泊位纬度2 BWWD2   FLOAT-->
			<xs:element name="LatitudeBerth2" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--泊位编号 BWBH   VARCHAR(9)-->
			<xs:element name="BerthNumber" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
