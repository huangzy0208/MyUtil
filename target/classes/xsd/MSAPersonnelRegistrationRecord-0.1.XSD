						<xs:maxLength value="7"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--数据来源 SJLY  CHAR(3)-->
			<xs:element name="DataSources">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TrainingInfo">
		<xs:sequence>
			<!--申报员编号   MembersDeclareNumber  char(18)-->
			<xs:element name="MembersDeclareNumber">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="18"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--序号   XH INT-->
			<xs:element name="No">
				<xs:simpleType>
					<xs:restriction base="xs:int"/>
				</xs:simpleType>
			</xs:element>
			<!--培训时间   PXSJ VARCHAR(40)-->
			<xs:element name="TrainingTime">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--培训地点   PXDD VARCHAR(100)-->
			<xs:element name="TrainingLocation">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--培训学时   PXXS VARCHAR(4)-->
			<xs:element name="TrainingHours">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--培训内容   PXNR VARCHAR(255)-->
			<xs:element name="TrainingContent">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--书面成绩   SMCJ VARCHAR(4)-->
			<xs:element name="WrittenScores">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--实践成绩   SJCJ VARCHAR(4)-->
			<xs:element name="PracticeResults">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--备注   BZ  VARCHAR(255)-->
			<xs:element name="Remark">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--更新标志 GXBZ CHAR(1)-->
			<xs:element name="UpdateFlag">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--更新时间 MRUT DATE-->
			<xs:element name="Updated" type="yyyyMMddHHmmss"/>
			<!--信息来源 XXLY CHAR(7)-->
			<xs:element name="InformationSources">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="7"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EachInfo">
		<xs:sequence>
			<xs:element name="BasicInfo" type="BasicInfo"/>
			<xs:element name="PersonnelRecord" type="PersonnelRecord"/>
			<xs:element name="TrainingInfoList">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TrainingInfo" type="TrainingInfo" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
