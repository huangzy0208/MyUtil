<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="urn:Declaration:datamodel:standard:CN:MSAShipShiftBerthDeclaration-0.1" targetNamespace="urn:Declaration:datamodel:standard:CN:MSAShipShiftBerthDeclaration-0.1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="Type/ConveyanceDataType.xsd"/>
	<xs:include schemaLocation="Type/DeclarationTypeCode.xsd"/>
	<xs:include schemaLocation="Type/MessageDefine.xsd"/>
	<xs:include schemaLocation="Type/MessageHead.xsd"/>
	<xs:element name="Message">
		<xs:annotation>
			<xs:documentation>报文</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MessageHead" type="Head"/>
				<xs:element name="MessageBody">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Declaration">
								<xs:annotation>
									<xs:documentation>海事船舶移泊申报(MSAShipShiftBerthDeclaration-0.1)</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Apply" type="Apply"/>
									</xs:sequence>
									<xs:attribute name="Xmlns" type="Xmlns" use="required" fixed="urn:Declaration:datamodel:standard:CN:MSAShipShiftBerthDeclaration-0.1">
										<xs:annotation>
											<xs:documentation>报文体命名空间</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="DataModelVersion" type="DataModelVersion" use="required" fixed="0.1">
										<xs:annotation>
											<xs:documentation>报文体数据元模型版本号</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="Version" type="Version" use="required" fixed="0.1">
										<xs:annotation>
											<xs:documentation>报文体版本号</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="Apply">
		<xs:sequence>
			<xs:element name="ApplyId">
				<xs:annotation>
					<xs:documentation>申报号</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="X16">
						<xs:length value="16"></xs:length>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="IMO">
				<xs:annotation>
					<xs:documentation>IMO编号</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="X25">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NameEn">
				<xs:annotation>
					<xs:documentation>英文船名</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="X35">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="InboundVoyage">
				<xs:annotation>
					<xs:documentation>进境(港)航次</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="X17">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OutboundVoyage">
				<xs:annotation>
					<xs:documentation>出境(港)航次</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="X17">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="StartPortCode">
				<xs:annotation>
					<xs:documentation>出发港代码</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="X11">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ArrivalPortCode">
				<xs:annotation>
					<xs:documentation>到达港代码</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="X11">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ArrivalDateTime">
				<xs:annotation>
					<xs:documentation>到港时间</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="yyyyMMddHHmmss"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MoveBetchDate">
				<xs:annotation>
					<xs:documentation>移泊时间</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="yyyyMMddHHmmss"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BerthCode">
				<xs:annotation>
					<xs:documentation>当前泊位代码</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="X35"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NewBerthCode">
				<xs:annotation>
					<xs:documentation>移至泊位代码</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="X35"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ApplyCode">
				<xs:annotation>
					<xs:documentation>申报人代码</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="X17">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ApplyName">
				<xs:annotation>
					<xs:documentation>申报人姓名</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="X35"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ApplyDate">
				<xs:annotation>
					<xs:documentation>申报日期</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="yyyyMMdd"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Memo">
				<xs:annotation>
					<xs:documentation>备注</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="X512"/>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
